{
  "id": "samsungce.robotCleanerOperatingState",
  "version": 1,
  "status": "proposed",
  "name": "Robot Cleaner Operating State",
  "ephemeral": false,
  "attributes": {
    "supportedOperatingState": {
      "schema": {
        "type": "object",
        "properties": {
          "value": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "idle",
                "homing",
                "cleaning",
                "charging",
                "chargingForRemainingJob",
                "charged",
                "drainingWater",
                "flexCharged",
                "moving",
                "paused",
                "error",
                "relocal",
                "descaling",
                "waitingForDescaling",
                "exploring",
                "emitDust",
                "monitoring",
                "monitoringAutomation",
                "patrol",
                "manual",
                "processing",
                "mediaPlaying",
                "messaging",
                "findingPet",
                "powerSaving",
                "reserved",
                "factoryReset",
                "calibrating",
                "welcoming",
                "detachingMopPad",
                "waitingForChangingMopPad",
                "attachingMopPad",
                "attachingMopPadForRemainingJob",
                "washingMop",
                "sterilizingMop",
                "dryingMop",
                "mopWashingPaused",
                "spinDrying",
                "preparingWater",
                "supplyingWater",
                "sabbath",
                "suspend"
              ]
            }
          }
        },
        "additionalProperties": false,
        "required": ["value"]
      },
      "enumCommands": []
    },
    "operatingState": {
      "schema": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string",
            "enum": [
              "idle",
              "homing",
              "cleaning",
              "charging",
              "chargingForRemainingJob",
              "charged",
              "drainingWater",
              "flexCharged",
              "moving",
              "paused",
              "error",
              "relocal",
              "descaling",
              "waitingForDescaling",
              "exploring",
              "emitDust",
              "monitoring",
              "monitoringAutomation",
              "patrol",
              "manual",
              "processing",
              "mediaPlaying",
              "messaging",
              "findingPet",
              "powerSaving",
              "reserved",
              "factoryReset",
              "calibrating",
              "welcoming",
              "detachingMopPad",
              "waitingForChangingMopPad",
              "attachingMopPad",
              "attachingMopPadForRemainingJob",
              "washingMop",
              "sterilizingMop",
              "dryingMop",
              "mopWashingPaused",
              "spinDrying",
              "preparingWater",
              "supplyingWater",
              "sabbath",
              "suspend"
            ]
          }
        },
        "additionalProperties": false,
        "required": ["value"]
      },
      "setter": "setOperatingState",
      "enumCommands": []
    },
    "cleaningStep": {
      "schema": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string",
            "enum": ["none", "mopping", "vacuuming", "oneStepCleaning"]
          }
        },
        "additionalProperties": false,
        "required": ["value"]
      },
      "enumCommands": []
    },
    "homingReason": {
      "schema": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string",
            "enum": [
              "none",
              "lowBattery",
              "cleaningCompleted",
              "cleaningCanceled",
              "relocalFailed",
              "dustFulled",
              "mapCreated",
              "doNotDisturb",
              "changingMopPad",
              "washingMop",
              "supplyingWater"
            ]
          }
        },
        "additionalProperties": false,
        "required": ["value"]
      },
      "enumCommands": []
    },
    "isMapBasedOperationAvailable": {
      "schema": {
        "type": "object",
        "properties": { "value": { "type": "boolean", "default": false } },
        "additionalProperties": false,
        "required": ["value"]
      },
      "enumCommands": []
    }
  },
  "commands": {
    "resume": { "name": "resume", "arguments": [] },
    "setOperatingState": {
      "name": "setOperatingState",
      "arguments": [
        {
          "name": "operatingState",
          "optional": false,
          "schema": {
            "type": "string",
            "enum": [
              "homing",
              "cleaning",
              "paused",
              "findingPet",
              "monitoringAutomation"
            ]
          }
        }
      ]
    },
    "returnToHome": { "name": "returnToHome", "arguments": [] },
    "start": { "name": "start", "arguments": [] },
    "pause": { "name": "pause", "arguments": [] },
    "cancelRemainingJob": { "name": "cancelRemainingJob", "arguments": [] }
  }
}
