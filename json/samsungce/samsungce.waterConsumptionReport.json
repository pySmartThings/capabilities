{
  "id": "samsungce.waterConsumptionReport",
  "version": 1,
  "status": "proposed",
  "name": "Water Consumption Report",
  "ephemeral": false,
  "attributes": {
    "waterConsumption": {
      "schema": {
        "type": "object",
        "properties": {
          "value": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "start": {
                "description": "Start date of the report period (Iso8601)",
                "type": "string",
                "pattern": "^(?:[1-9]\\d{3}-?(?:(?:0[1-9]|1[0-2])-?(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-?(?:29|30)|(?:0[13578]|1[02])-?31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-?02-?29)T(?:[01]\\d|2[0-3]):?[0-5]\\d:?[0-5]\\d(?:\\.\\d{3})?(?:Z|[+-][01]\\d(?::?[0-5]\\d)?)$"
              },
              "end": {
                "description": "End date of the report period (Iso8601)",
                "type": "string",
                "pattern": "^(?:[1-9]\\d{3}-?(?:(?:0[1-9]|1[0-2])-?(?:0[1-9]|1\\d|2[0-8])|(?:0[13-9]|1[0-2])-?(?:29|30)|(?:0[13578]|1[02])-?31)|(?:[1-9]\\d(?:0[48]|[2468][048]|[13579][26])|(?:[2468][048]|[13579][26])00)-?02-?29)T(?:[01]\\d|2[0-3]):?[0-5]\\d:?[0-5]\\d(?:\\.\\d{3})?(?:Z|[+-][01]\\d(?::?[0-5]\\d)?)$"
              },
              "cumulativeAmount": {
                "description": "Accumulated water consumption (ml) during the reporting period",
                "type": "number"
              },
              "delta": {
                "description": "Difference of current and previous consumptionAmount",
                "type": "number"
              }
            }
          }
        },
        "additionalProperties": false,
        "required": ["value"]
      },
      "enumCommands": []
    }
  },
  "commands": {}
}
